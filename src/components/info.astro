---
import '../styles/global.css';
import Navbar from "./nav.astro"

//parts
import Sass from "./sass.astro";
import Lanc from "./lanc.astro";
import Saga from "./saga.astro";
import Binz from "./binz.astro";
import Liet from "./liet.astro";
import Berg from "./berg.astro";
import Tesc from "./tesc.astro";
import Samt from "./samt.astro";
import Ramb from "./ramb.astro";
import Alte from "./alte.astro";
import Rueg from "./rueg.astro";
import Stral from "./stral.astro";
import Safe from "./safe.astro";
import Putb from "./putb.astro";
import Laut from "./laut.astro";
import Lamo from "./lamo.astro";
import Beuc from "./beuc.astro";
import Sera from "./sera.astro";
import Pose from "./pose.astro";
import Seel from "./seel.astro";
import Bilb from "./bilb.astro";
import Jagd from "./jagd.astro";
import Garf from "./garf.astro";
import Selw from "./selw.astro";
import Selo from "./selo.astro";
import Baab from "./baab.astro";
import Phil from "./phil.astro";
import Goeh from "./goeh.astro";
---
<!doctype html>
<html lang="en" class="no-js">
	<head>
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<meta name="description" content="Trains of Ruegen" />
		<title>Trains of Ruegen</title>
		<meta charset="UTF-8" />
	</head>
	<body>
		<main>
			<Navbar />
			<h1 class="center re9" id="9">
				RE9 <br /> Stralsund - Sassnitz; Baltic resort Binz
			</h1>
			<div class="items" id="info">
				<div id="9sass">
					<Sass />
				</div>
				<div id="9lanc">
					<Lanc />
				</div>
				<div id="9saga">
					<Saga />
				</div>
				<div id="9binz">
					<Binz />
				</div>
				<div id="9liet">
					<Liet />
				</div>
				<div id="9berg">
					<Berg />
				</div>
				<div id="9tesc">
					<Tesc />
				</div>
				<div id="9samt">
					<Samt />
				</div>
				<div id="9ramb">
					<Ramb />
				</div>
				<div id="9alte">
					<Alte />
				</div>
				<div id="9rueg">
					<Rueg />
				</div>
				<div id="9stral">
					<Stral />
				</div>
			</div>
			<h1 class="center re27" id="27">
				RE27 <br /> Bergen(Ruegen) - Sassnitz Ferry terminal
			</h1>
			<div class="items">
				<div id="27berg">
					<Berg />
				</div>
				<div id="27safe">
					<Safe />
				</div>
			</div>
			<h1 class="center zugebringer" id="z">
				Zu|g|ebringer <br /> Bergen(Ruegen) - Lauterbach Pier
			</h1>
			<div class="items">
				<div id="zberg">
					<Berg />
				</div>
				<div id="zputb">
					<Putb />
				</div>
				<div id="zlaut">
					<Laut />
				</div>
				<div id="zlamo">
					<Lamo />
				</div>
			</div>
			<h1 class="center roland" id="P">
				Racing Roland <br /> Lauterbach Pier - Goehren
			</h1>
			<div class="items">
				<div id="Plamo">
					<Lamo />
				</div>
				<div id="Plaut">
					<Laut />
				</div>
				<div id="Pputb">
					<Putb />
				</div>
				<div id="Pbeuc"><Beuc /></div>
				<div id="Ppose"><Pose /></div>
				<div id="Pseel"><Seel /></div>
				<div id="Psera"><Sera /></div>
				<div id="Pbilb"><Bilb /></div>
				<div id="Pjagd"><Jagd /></div>
				<div id="Pgarf"><Garf /></div>
				<div id="Pselw"><Selw /></div>
				<div id="Pselo"><Selo /></div>
				<div id="Pbaab"><Baab /></div>
				<div id="Pphil"><Phil /></div>
				<div id="Pgoeh"><Goeh /></div>
			</div>
		</main></body
	>
</html>
